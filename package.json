    "@types/jest": "^29.4.0",
    "jest": "^29.4.3",
    "parcel": "^2.8.3",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Animus-Blue/data-cruncher.git"
